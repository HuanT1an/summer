#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int a[100];
	int i,j,t;
	int len;
	printf("输入要输入的数字的个数：");
	scanf("%d",&len);
	for(i=0;i<len;i++)
	{
		scanf("%d",&a[i]);
	} 
	for(j=0;j<len-1;j++)
	{
		for(i=0;i<len-1-j;i++)
		{
			if(a[i]>a[i+1])
			{
				t=a[i];
				a[i]=a[i+1];
				a[i+1]=t;
			}
		}
	}
	for(i=0;i<len;i++)
	{
		printf("%d ",a[i]);
	}
	return 0;
}
