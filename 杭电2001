#include <stdio.h>
#include <stdlib.h>
#include <math.h> 

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	double a,b,c,d,s;
	while(scanf("%lf %lf %lf %lf",&a,&b,&c,&d)!=EOF)
	{
		s=sqrt((a-c)*(a-c)+(b-d)*(b-d));
		printf("%.2lf\n",s);
	}
	return 0;
}
