#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int n;
	while(scanf("%d",&n)!=EOF)
	{
		if(n>100||n<0)
		printf("Êý¾ÝÓÐÎó\n");
		else if(n>=90)
		printf("A\n");
		else if(n>=80)
		printf("B\n");
		else if(n>=70)
		printf("C\n");
		else if(n>=60)
		printf("D\n");
		else
		printf("E\n");
	} 
	return 0;
}
