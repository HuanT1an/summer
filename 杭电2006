#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int n,s,i,a;
	while(scanf("%d",&n)!=EOF)
	{
		s=1;
		for(i=0;i<n;i++)
		{
			scanf("%d" ,&a);
			if(a%2==1)
			s=s*a;
		}
		printf("%d\n",s);
	}
	return 0;
}
