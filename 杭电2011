#include <stdio.h>
#include <stdlib.h>
#include<math.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	double m,n,i,j,s;
	while(scanf("%lf",&m)!=EOF)
	{
		for(i=0;i<m;i++)
		{
			s=0;
			scanf("%lf",&n);
			for(j=1;j<=n;j++)
			{
			  s=s+1/j*pow(-1,j+1);
			}
			printf("%.2lf\n",s);
		}
		
	}
	return 0;
}
