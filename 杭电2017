#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int num,i,n;
	char line;
	scanf("%d",&n);
	getchar();
	for(i=1;i<=n;i++)
	{
		num=0;
		for(;(line=getchar())!='\n';)
		{
			if(line>='0'&&line<='9')
			num++;
		}
		printf("%d\n",num);
	}
	return 0;
}
