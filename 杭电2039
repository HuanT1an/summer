#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	double a,b,c,n,i;
	while(scanf("%lf",&n)!=EOF)
	{
		for(i=0;i<n;i++)
		{
			scanf("%lf%lf%lf",&a,&b,&c);
			if((a+b)>c&&(b+c)>a&&(a+c)>b)
			printf("是三角形\n");
			else
			printf("不是三角形\n");
		}
	} 
	return 0;
}
