#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int n,t;
    float num,max;
    scanf("%d",&t);
    while(t--)
	{
        scanf("%d",&n);
        if(n>0)
            scanf("%f",&max);
        while(--n)
		{
            scanf("%f",&num);
            if(max<num)
                max=num;
        }
        printf("%.2f\n",max);
    }
	return 0;
}
